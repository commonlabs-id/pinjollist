{
  "name": "pinjollist",
  "alias": "pinjollist.now.sh",
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/www/$1"
    }
  ],
  "builds": [
    { "src": "api/**/*.ts", "use": "@now/node@canary" },
    { "src": "www/package.json", "use": "@now/next" }
  ]
}
