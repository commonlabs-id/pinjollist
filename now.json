{
  "name": "pinjollist",
  "alias": "pinjollist.now.sh",
  "version": 2,
  "routes": [
    { "src": "/api/(.*).ts", "dest": "/api/routes/$1" },
    {
      "src": "/(.*)",
      "dest": "/www/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ],
  "builds": [
    { "src": "api/routes/**/*.ts", "use": "@now/node@canary" },
    { "src": "www/package.json", "use": "@now/next" }
  ]
}
