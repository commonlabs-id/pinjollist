{
  "extends": "../.eslintrc.js",
  "rules": {
    "camelcase": "off",
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": true,
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ],
    "import/no-dynamic-require": "off"
  }
}
